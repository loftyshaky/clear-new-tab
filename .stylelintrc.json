{
    "extends": ["stylelint-config-standard-scss", "stylelint-config-prettier-scss"],
    "rules": {
        "selector-class-pattern": "^([a-z][a-z0-9_]*)([a-z0-9]+)*$",
        "scss/dollar-variable-pattern": "^([a-z][a-z0-9_]*)([a-z0-9]+)*$",
        "scss/at-mixin-pattern": "^([a-z][a-z0-9_]*)([a-z0-9]+)*$",
        "scss/at-function-pattern": "^([a-z][a-z0-9_]*)([a-z0-9]+)*$",
        "keyframes-name-pattern": "^([a-z][a-z0-9_]*)([a-z0-9]+)*$",
        "custom-property-pattern": "^([a-z][a-z0-9_]*)([a-z0-9]+)*$",
        "selector-id-pattern": "^([a-z][a-z0-9_]*)([a-z0-9]+)*$",
        "max-nesting-depth": 4,
        "color-named": null,
        "selector-no-qualifying-type": null,
        "selector-max-compound-selectors": null,
        "selector-max-id": null
    }
}
